{
  "name": "moonne-backend",
  "version": "1.0.0",
  "description": "Backend server cho ứng dụng Moonne",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prod": "NODE_ENV=production node server.js",
    "pm2": "pm2 start server.js --name moonne-backend",
    "pm2:stop": "pm2 stop moonne-backend",
    "pm2:restart": "pm2 restart moonne-backend"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": ["moonne", "backend", "api"],
  "author": "Moonne Team",
  "license": "MIT"
}
